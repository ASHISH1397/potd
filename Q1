class Solution{
public:
	int print2largest(int arr[], int n) {
	    
	    int max=INT_MIN;
	    int secMax=INT_MIN;
	    
	    for(int i=0;i<n;i++){
	        if(arr[i]>max){
	            secMax=max;
	            max=arr[i];
	            
	        }
	        else if(arr[i]>secMax && arr[i]!=max) secMax=arr[i];
	    }
	    return secMax==INT_MIN ? -1 : secMax ;
	}
};
